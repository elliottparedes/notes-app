    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "dev": "cross-env NODE_ENV=development nuxt dev",
    "dev:quiet": "cross-env VITE_PLUGIN_PWA_LOG_LEVEL=silent nuxt dev",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:watch": "vue-tsc --noEmit --watch",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test:pwa": "npm run build && npm run preview",
    "postinstall": "nuxt prepare",
    "clean": "rimraf .nuxt node_modules/.vite node_modules/.cache",
    "fresh": "npm run clean && npm run dev",
    "migrate": "node migrations/run-migration.js"
  },
